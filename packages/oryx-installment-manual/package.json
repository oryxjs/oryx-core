{
  "name": "oryx-installment-manual",
  "version": "0.0.0",
  "description": "Installment plugin for oryx projects.",
  "author": "Aboozaid",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "peerDependencies": {
    "@oryxjs/oryx": "^1.3.5",
    "oryx-interfaces": "^1.3.2",
    "typeorm": "^0.2.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-typescript": "^7.18.6",
    "babel-preset-oryx-package": "^1.1.19",
    "cross-env": "^5.2.1",
    "oryx-tsconfig": "^1.0.0",
    "typescript": "^4.7.4"
  }
}
